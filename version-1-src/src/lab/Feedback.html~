<script>
function prefill()
{
	// Domain prefill
	var url = window.location.href
	
	var domain=location.search.split('domain=')[1]
	var lab=location.search.split('lab=')[1]
	document.getElementById("element_8").value=domain
	document.getElementById("element_9").value="Computer Graphics"
	
	// Date prefill
	var date = new Date().toISOString().substring(0, 10);
	field = document.querySelector('#element_5_1');
	field.value = date.substring(8, 10);
	field = document.querySelector('#element_5_2');
	field.value = date.substring(5, 7);
	field = document.querySelector('#element_5_3');
	field.value = date.substring(0, 4);
}
</script>
 
